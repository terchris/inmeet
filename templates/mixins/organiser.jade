mixin organiser(org)
	.organiser
		.organiser__header
			.img-wrapper
				a(href=org.url): img(src=org.image, alt=org.name, width=40, height=40).img-circle
			h4.organiser__name 
				a(href=org.url) #{org.name}				
		p.mv-0=org.title
